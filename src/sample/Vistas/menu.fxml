<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="rootPane" focusTraversable="true" nodeOrientation="LEFT_TO_RIGHT" prefHeight="650.0"
            prefWidth="1100.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controladores.MenuController">
   <children>
      <Label layoutX="338.0" layoutY="94.0" prefHeight="42.0" prefWidth="432.0" text="SISTEMA DE ASIGNACIÓN DE AULAS"
             textFill="WHITE" textOverrun="CLIP">
         <font>
            <Font name="Roboto Medium" size="23.0" />
         </font>
      </Label>
      <ImageView fitHeight="157.0" fitWidth="178.0" layoutX="34.0" layoutY="28.0" pickOnBounds="true"
                 preserveRatio="true">
         <image>
            <Image url="@../icons/logoUnicahV3.png"/>
         </image>
      </ImageView>
      <HBox fx:id="boxMenus" layoutX="227.0" layoutY="227.0" prefHeight="223.0" prefWidth="654.0" spacing="20.0">
         <children>
            <AnchorPane fx:id="paneIngresar" onMouseClicked="#ingresoDatos" prefHeight="227.0" prefWidth="207.0" styleClass="menus">
               <children>
                  <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="60.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/1.png" />
                     </image>
                  </ImageView>
                  <Label alignment="CENTER" layoutX="39.0" layoutY="133.0" text="Ingresar Datos" textFill="WHITE">
                     <font>
                        <Font name="Roboto Black" size="20.0" />
                     </font>
                  </Label>
                  <Label alignment="CENTER" layoutX="18.0" layoutY="163.0" prefHeight="51.0" prefWidth="171.0" text="Importar datos a la base de datos" textAlignment="CENTER" textFill="WHITE" wrapText="true">
                     <font>
                        <Font name="Roboto Medium" size="11.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
            <AnchorPane fx:id="paneAsignacion" onMouseClicked="#asignoAulas" prefHeight="227.0" prefWidth="207.0" styleClass="menus">
               <children>
                  <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="54.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/2.png" />
                     </image>
                  </ImageView>
                  <Label alignment="CENTER" layoutX="15.0" layoutY="133.0" text="Asignación de Aulas" textFill="WHITE">
                     <font>
                        <Font name="Roboto Black" size="20.0" />
                     </font>
                  </Label>
                  <Label alignment="CENTER" layoutX="18.0" layoutY="163.0" prefHeight="51.0" prefWidth="171.0" text="Asignar las aulas para cada clase" textAlignment="CENTER" textFill="WHITE" textOverrun="CLIP" wrapText="true">
                     <font>
                        <Font name="Roboto Medium" size="11.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
            <AnchorPane fx:id="paneModificacion" onMouseClicked="#modificarDatos" prefHeight="211.0" prefWidth="207.0" styleClass="menus">
               <children>
                  <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="54.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/3.png" />
                     </image>
                  </ImageView>
                  <Label alignment="CENTER" layoutX="47.0" layoutY="133.0" text="Modificación" textFill="WHITE">
                     <font>
                        <Font name="Roboto Black" size="20.0" />
                     </font>
                  </Label>
                  <Label alignment="CENTER" layoutX="18.0" layoutY="164.0" prefHeight="51.0" prefWidth="171.0" text="Hacer modificaciones en la asignación de aulas" textAlignment="CENTER" textFill="WHITE" wrapText="true">
                     <font>
                        <Font name="Roboto Medium" size="11.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
         </children>
      </HBox>
      <ImageView fitHeight="51.0" fitWidth="38.0" layoutX="1042.0" layoutY="9.0" onMouseClicked="#goTo"
                 pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/equipo.png"/>
         </image>
      </ImageView>
   </children>
   <styleClass>
      <String fx:value="gradient-light"/>
      <String fx:value="arrow"/>
   </styleClass>
</AnchorPane>
